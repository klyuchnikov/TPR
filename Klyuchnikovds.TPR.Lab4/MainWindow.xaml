<Window x:Class="Klyuchnikovds.TPR.Lab4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Lab 4 'Динамическое программирование'"
        Height="400"
        Width="750">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="45" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Button Content="Загрузить карту"
                Height="23"
                HorizontalAlignment="Left"
                Name="button1"
                VerticalAlignment="Top"
                Width="91"
                Click="button1_Click"
                Margin="12,12,0,0" />
        <ScrollViewer HorizontalScrollBarVisibility="Auto"
                      Grid.Row="1"
                      VerticalScrollBarVisibility="Auto">
            <Canvas Name="canvas">
            </Canvas>
        </ScrollViewer>
    </Grid>
</Window>
